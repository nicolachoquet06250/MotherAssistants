@html.extend('layout', function(model) {
	@html.block('page_title_ma', function(model) {
		<title> MotherAssistants - AM </title>
	})

	@html.block('page_title_parent', function(model) {
		<title> MotherAssistants - Parents </title>
	})

	@html.block('page_title_anonymous', function(model) {
		<title> MotherAssistants </title>
	})
	@html.block('content_anonymous', function(model) {
		<div class="container-fluid">
			<div class="row">
				<div class="section white">
					<div class="row container">
						<div class="col-12">
							<h1><img src="/images/logo-web-transparent.png" alt="@model.title" class="responsive-img" /></h1>
							<p>
								@{ var presentation = model.app.presentation }
								@presentation.forEach(function(item){
									@item<br>
								})
							</p>
						</div>
					</div>
				</div>
				<div class="parallax-container">
					<div class="parallax">
						<img src="/images/mon-enfant-ne-mange-que-chez-la-nounou.jpg" />
					</div>
				</div>
				<div class="section white">
					<div class="row container">
						<div class="col-12">
							<h2><img src="/images/logo-web-transparent.png" alt="@model.title" class="responsive-img" /></h2>
							<p>
								@{ var presentation = model.app.presentation }
									@presentation.forEach(function(item){
										@item<br>
									})
							</p>
						</div>
					</div>
				</div>
			</div>
		</div>
	})

